  // });\n  // }\n  async created() {\n    let obj = new Object();\n    let bookObj = new Object();\n    var orders = [];\n    await firebase.firestore().collection(\"Users\").doc(this.id).get().then(snapshot => {\n      orders = snapshot.data().orders;\n      obj['shipTo'] = snapshot.data().name;\n      obj['addressline1'] = snapshot.data().addrLine1;\n      obj['addressline2'] = snapshot.data().addrLine2;\n      obj['city'] = snapshot.data().city;\n      obj['landmark'] = snapshot.data().landmark;\n      obj['state'] = snapshot.data().state;\n      obj['pincode'] = snapshot.data().zipcode; // console.log(orders)\n    });\n    orders.forEach(async order => {\n      obj['orderID'] = order;\n      await firebase.firestore().collection(\"Orders\").doc(order).get().then(snapshot => {\n        //  console.log(snapshot.data())  \n        obj['orderDate'] = snapshot.data().orderDate;\n        var timestamp = snapshot.data().expectedDate;\n        var time = new Date(timestamp.seconds * 1000);\n        var month = time.getMonth() < 10 ? \"0\" + time.getMonth() : time.getMonth();\n        var date = time.getDate() < 10 ? \"0\" + time.getDate() : time.getDate();\n        var year = time.getFullYear();\n        obj['bookDelivery'] = date + \"-\" + month + \"-\" + year;\n        obj['orderCompleted'] = snapshot.data().orderCompleted;\n        snapshot.data().books.forEach(async book => {\n          // console.log(book);\n          // console.log(obj);\n          await firebase.firestore().collection('books').doc(book).get().then(snapshot => {\n            this.orderDetailsArray[this.index] = {};\n            this.orderDetailsArray[this.index]['bookName'] = snapshot.data().name;\n            this.orderDetailsArray[this.index]['bookPrice'] = snapshot.data().price;\n            this.orderDetailsArray[this.index]['bookImageURL'] = snapshot.data().url;\n            this.orderDetailsArray[this.index]['bookYear'] = snapshot.data().publicationYear;\n            this.orderDetailsArray[this.index]['bookGenre'] = snapshot.data().genre || \"\";\n            this.orderDetailsArray[this.index]['bookAuthor'] = snapshot.data().author || \"\";\n          }).finally(() => {\n            this.orderDetailsArray[this.index].shipTo = obj.shipTo;\n            this.orderDetailsArray[this.index].addressline1 = obj.addressline1;\n            this.orderDetailsArray[this.index].addressline2 = obj.addressline2;\n            this.orderDetailsArray[this.index].city = obj.city;\n            this.orderDetailsArray[this.index].state = obj.state;\n            this.orderDetailsArray[this.index].orderID = obj.orderID;\n            this.orderDetailsArray[this.index].pincode = obj.pincode;\n            this.orderDetailsArray[this.index].orderDate = obj.orderDate;\n            this.orderDetailsArray[this.index].bookDelivery = obj.bookDelivery;\n            this.orderedDetails.push(this.orderDetailsArray[this.index++]);\n          });\n          bookObj = new Object();\n        });\n      });\n    });\n    console.log(this.orderedDetails);\n  }\n\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,MAAP,MAAmB,oCAAnB;AACA,OAAOC,MAAP,MAAmB,oCAAnB;AACA,OAAOC,WAAP,MAAwB,2CAAxB;AACA,OAAOC,UAAP,MAAuB,wCAAvB;AACA,OAAOC,YAAP,MAAyB,4CAAzB;AACA,OAAOC,OAAP,MAAoB,oDAApB;AACA,OAAOC,YAAP,MAAyB,yDAAzB;AACA,OAAOC,YAAP,MAAyB,yDAAzB;AACA,OAAOC,QAAP,MAAqB,qBAArB;AACA,eAAc;EACVC,IAAI,GAAE;IACF,OAAM;MACFC,cAAc,EAAE,EADd;MAEFC,iBAAiB,EAAE,EAFjB;MAGFC,KAAK,EAAE,CAHL;MAIFC,EAAE,EAAE,KAAKC,MAAL,CAAYC,KAAZ,CAAkBF;IAJpB,CAAN;EAMH,CARS;;EASVG,UAAU,EAAC;IACXhB,MADW;IAEXC,MAFW;IAGXC,WAHW;IAIXC,UAJW;IAKXC,YALW;IAMXC,OANW;IAOXC,YAPW;IAQXC;EARW,CATD;;EAmBV;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EAEA;EAEA;EACA;EACA;EACA;EAEA;EAEA;EACA,MAAMU,OAAN,GAAe;IACX,IAAIC,GAAE,GAAI,IAAIC,MAAJ,EAAV;IACA,IAAIC,OAAM,GAAI,IAAID,MAAJ,EAAd;IACA,IAAIE,MAAK,GAAI,EAAb;IACA,MAAMb,QAAQ,CAACc,SAAT,GAAqBC,UAArB,CAAgC,OAAhC,EAAyCC,GAAzC,CAA6C,KAAKX,EAAlD,EAAsDY,GAAtD,GAA4DC,IAA5D,CAAkE