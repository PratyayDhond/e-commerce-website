s.checkoutBooksPrice.get(bookID) * this.checkoutBooks.get(bookID));\n        amount += this.checkoutBooksPrice.get(bookID) * this.checkoutBooks.get(bookID);\n        quantity += this.checkoutBooks.get(bookID);\n      }); // console.log(amount)\n      // console.log(quantity)\n      // console.log(new Date());\n      // console.log();\n\n      var books = [];\n      this.checkoutBooksUnique.forEach(item => {\n        books.push(item);\n      }); // console.log(books)\n      // BOOKMARK: PAYWALL GOES HERE\n\n      var total = amount + this.deliv * quantity;\n      let orderID = \"\";\n      await firebase.firestore().collection(\"Orders\").add({\n        books: books,\n        quantity: quantity,\n        amount: amount,\n        total: total,\n        shippingCharges: this.deliv,\n        orderDate: new Date(),\n        // BOOKMARK: expected delivery to 14 days from order time.\n        expectedDate: new Date(Date.now() + 12096e5),\n        packageNo: \"\",\n        discount: 0\n      }).then(snapshot => {\n        orderID = snapshot.id; // console.log(snapshot);\n        // console.log(snapshot.id)\n      }); // firebase.firestore().collection(\"Orders\").add({\n\n      console.log(orderID);\n      var userData;\n      firebase.firestore().collection(\"Users\").doc(this.userID).get(snapshot => {\n        console.log(snapshot.data());\n        userData = snapshot.data();\n      });\n      console.log(userData); // })\n    },\n\n    calculateTotal(quantity) {\n      this.items += quantity;\n      this.cost += this.prices[this.prices.length - 1];\n      console.log(\"current cost : \" + this.cost);\n    },\n\n    calculate(price, quantity, bookID) {\n      this.prices.push(price);\n      console.log(bookID);\n      this.calculateTotal(quantity);\n      this.checkoutBooksUnique.add(bookID);\n      var count = this.checkoutBooks.get(bookID) || 0; // Checking for if book's quantity is decremented or incremented.\n\n      if (quantity === 1) this.checkoutBooks.set(bookID, count + 1);else if (quantity === -1) {\n        if (count > 0) this.checkoutBooks.set(bookID, count - 1);\n      }\n      if (price > 0) this.checkoutBooksPrice.set(bookID, price);\n      console.log(this.checkoutBooks);\n      console.log(this.checkoutBooksUnique);\n      console.log(this.checkoutBooksPrice);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,MAAP,MAAmB,oCAAnB;AACA,OAAOC,MAAP,MAAmB,oCAAnB;AACA,OAAOC,WAAP,MAAwB,2CAAxB;AACA,OAAOC,UAAP,MAAuB,wCAAvB;AACA,OAAOC,YAAP,MAAyB,uCAAzB;AACA,OAAOC,SAAP,MAAsB,uCAAtB;AACA,OAAOC,YAAP,MAAyB,kCAAzB;AACA,OAAOC,QAAP,MAAqB,qBAArB;AAEA,eAAc;EACZ;EACCC,IAAI,GAAE;IACJ,OAAM;MACJC,IAAI,EAAE,CADF;MAEJC,KAAK,EAAE,CAFH;MAGJC,MAAM,EAAE,EAHJ;MAIHC,GAAG,EAAE,CAJF;MAKH;MACA;MACCC,KAAK,EAAE,EAPL;MAQHC,MAAM,EAAE,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,EARvB;MASHC,KAAK,EAAE,EATJ;MAUHC,mBAAmB,EAAE,IAAIC,GAAJ,EAVlB;MAWHC,aAAa,EAAE,IAAIC,GAAJ,EAXZ;MAYHC,kBAAkB,EAAE,IAAID,GAAJ;IAZjB,CAAN;EAcD,CAjBU;;EAkBX,MAAME,OAAN,GAAgB;IACX,MAAMC,EAAC,GAAIlB,QAAQ,CAACmB,SAAT,EAAX,CADW,CAEX;;IACAC,OAAO,CAACC,GAAR,CAAY,yBAAZ;;IAEA,IAAG;MAEDH,EAAE,CAACI,UAAH,CAAc,OAAd,EAAuBC,GAAvB,CAA2B,KAAKhB,MAAhC,EAAwCiB,GAAxC,GAA8CC,IAA9C,CAAoDC,CAAD,IAAO;QACxDN,OAAO,CAACC,GAAR,CAAYK,CAAC,CAACzB,IAAF,EAAZ;QACA,IAAI0B,GAAE,GAAID,CAAC,CAACzB,IAAF,GAAS2B,IAAnB;QACAR,OAAO,CAACC,GAAR,CAAYM,GAAZ;;QAEA,KAAI,IAAIE,IAAI,CAAZ,EAAeA,IAAIF,GAAG,CAACG,MAAvB,EAA+BD,CAAC,EAAhC,EAAmC;UACjCX,EAAE,CAACI,UAAH,CAAc,OAAd,EAAuBC,GAAvB,CAA2BI,GAAG,CAACE,CAAD,CAA9B,EAAmCL,GAAnC,GAAyCC,IAAzC,CAA+CM,IAAD,IAAU;YACtD,IAAIC,IAAG,GAAID,IAAI,CAAC9B,IAAL,EAAX;YACA+B,IAAI,CAACtB,EAAL,GAAUiB,GAAG,CAACE,CAAD,CAAb;YACA,KAAKlB,KAAL,CAAWsB,IAAX,CAAgBD,IAAhB;UACD,CAJD;QAKF;;QACAZ,OAAO,CAACC,GAAR,CAAY,sBAAZ;QACED,OAAO,CAACC,GAAR,CAAY,KAAKV,KAAjB;MAEH,CAfD;IAgBF,CAlBA,CAoBA,OAAMuB,CAAN,EAAQ;MACJd,OAAO,CAACC,GAAR,CAAYa,CAAZ;IACJ;EACH,CA9CS;;EAgDVC,UAAU,EAAC;IACX1C,MADW;IAEXC,MAFW;IAGXC,WAHW;IAIXC,UAJW;IAKXC,YALW;IAMXC;EANW,CAhDD;EAwDdsC,OAAO,EAAE;IACP,MAAMC,QAAN,GAAiB;MACfjB,OAAO,CAACC,GAAR,CAAY,0BAAZ;MACA,IA