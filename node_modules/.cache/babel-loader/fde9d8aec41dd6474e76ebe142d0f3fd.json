{"ast":null,"code":"import Header from '../../components/Header/Header.vue';\nimport Router from 'Router.vue';\nexport default {\n  data() {\n    return {\n      name: '',\n      email: '',\n      nameCheck: false,\n      mailCheck: false\n    };\n  },\n\n  methods: {\n    isNameValid() {\n      this.name = this.name.trim();\n      console.log('isNameValid called');\n\n      try {\n        if (this.name === '') {\n          alert('name cannot be empty');\n          this.nameCheck = false;\n        } else {\n          //Checking if the string has literals between a-z, A-Z or a blank space\n          var val = /^[a-zA-Z ]+$/.test(this.name);\n\n          if (val) {\n            //checking if there is any space in the user name,\n            //indicating that the user has written name as First Name and Last Name\n            var hasSpace = this.checkFor(' ', this.name.trim()); // for(let i = 0; i < this.name.trim().length; i++){\n            //   if(this.name[i] == ' '){\n            //     hasSpace = true;\n            //     break;\n            //   }\n            // }\n\n            if (hasSpace) {\n              console.log('Name is valid');\n              this.nameCheck = true;\n              this.isEmailValid();\n            } else {\n              console.log('Name is invalid');\n              alert(' Enter your full name');\n            }\n          } else {\n            console.log('Name is invalid');\n            alert('Enter a valid name');\n          }\n        }\n      } catch (e) {\n        console.log(e);\n      }\n    },\n\n    isEmailValid() {\n      this.email = this.email.trim(); //Checking if the string has literals between a-z, A-Z or a blank space\n\n      console.log('isemailValid called');\n      var val = /^[a-zA-Z0-9@.+]+$/.test(this.email.trim());\n\n      if (val) {\n        console.log('email contains valid characters so far');\n        var hasAtTheRate = this.checkFor('@', this.email);\n        var hasDot = this.checkFor('.', this.email);\n        var toAlert = false;\n\n        if (hasAtTheRate) {\n          if (hasDot) {\n            console.log('email is valid');\n            this.mailCheck = true;\n          } else {\n            toAlert = true;\n          }\n        } else {\n          toAlert = true;\n        } // for(let i = 0; i < this.email.length; i++){\n\n\n        if (toAlert) {\n          alert('Enter A Valid Email');\n        }\n      } else {\n        console.log('Invalid email');\n        this.email = '';\n        alert('Enter a valid email');\n      }\n    },\n\n    checkFor(s, str) {\n      for (let i = 0; i < str.length; i++) {\n        if (str[i] === s) return true;\n      }\n\n      return false;\n    },\n\n    pushData() {\n      Router.push({});\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,MAAP,MAAmB,oCAAnB;AACA,OAAOC,MAAP,MAAmB,YAAnB;AACA,eAAc;EACZC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,KAAK,EAAE,EAFF;MAGLC,SAAS,EAAE,KAHN;MAILC,SAAS,EAAE;IAJN,CAAP;EAMD,CARW;;EASZC,OAAO,EAAE;IACPC,WAAW,GAAE;MACX,KAAKL,IAAL,GAAY,KAAKA,IAAL,CAAUM,IAAV,EAAZ;MACEC,OAAO,CAACC,GAAR,CAAY,oBAAZ;;MACA,IAAG;QACH,IAAG,KAAKR,IAAL,KAAc,EAAjB,EAAoB;UAClBS,KAAK,CAAC,sBAAD,CAAL;UACA,KAAKP,SAAL,GAAiB,KAAjB;QACD,CAHD,MAGK;UACH;UACA,IAAIQ,GAAE,GAAI,eAAeC,IAAf,CAAoB,KAAKX,IAAzB,CAAV;;UACA,IAAGU,GAAH,EAAO;YAEJ;YACC;YACA,IAAIE,QAAO,GAAI,KAAKC,QAAL,CAAc,GAAd,EAAkB,KAAKb,IAAL,CAAUM,IAAV,EAAlB,CAAf,CAJG,CAMH;YACA;YACA;YACA;YACA;YACA;;YAEF,IAAGM,QAAH,EAAY;cACVL,OAAO,CAACC,GAAR,CAAY,eAAZ;cACA,KAAKN,SAAL,GAAiB,IAAjB;cACA,KAAKY,YAAL;YACD,CAJD,MAIK;cACHP,OAAO,CAACC,GAAR,CAAY,iBAAZ;cACAC,KAAK,CAAC,uBAAD,CAAL;YACF;UACD,CArBD,MAqBK;YACHF,OAAO,CAACC,GAAR,CAAY,iBAAZ;YACAC,KAAK,CAAC,oBAAD,CAAL;UACF;QACF;MACD,CAjCC,CAiCD,OAAMM,CAAN,EAAQ;QACPR,OAAO,CAACC,GAAR,CAAYO,CAAZ;MACF;IACD,CAxCM;;IAyCPD,YAAY,GAAE;MACZ,KAAKb,KAAL,GAAa,KAAKA,KAAL,CAAWK,IAAX,EAAb,CADY,CAEZ;;MACIC,OAAO,CAACC,GAAR,CAAY,qBAAZ;MACA,IAAIE,GAAE,GAAI,oBAAoBC,IAApB,CAAyB,KAAKV,KAAL,CAAWK,IAAX,EAAzB,CAAV;;MACA,IAAGI,GAAH,EAAO;QACLH,OAAO,CAACC,GA