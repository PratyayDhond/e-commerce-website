 this.cost += this.prices[this.prices.length - 1];\n      console.log(\"current cost : \" + this.cost); // }\n    },\n\n    calculate(price, quantity) {\n      this.prices.push(price); // console.log(this.prices)\n      // this.cost = this.cost + price;\n      // this.items = this.items + quantity\n\n      this.calculateTotal(quantity); // console.log(price)\n      // console.log(quantity)\n    } //  decrement(price){\n    //   var p = parseInt(price);\n    //   this.cost -= p;\n    //   this.items -= quantity\n    //   console.log(this.cost)\n    // },\n    // increment(price){\n    //   var p = parseInt(price);\n    //   this.cost += p;\n    //   this.items += quantity\n    //   console.log(this.cost)\n    // }\n\n\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,MAAP,MAAmB,oCAAnB;AACA,OAAOC,MAAP,MAAmB,oCAAnB;AACA,OAAOC,WAAP,MAAwB,2CAAxB;AACA,OAAOC,UAAP,MAAuB,wCAAvB;AACA,OAAOC,YAAP,MAAyB,uCAAzB;AACA,OAAOC,SAAP,MAAsB,uCAAtB;AACA,OAAOC,QAAP,MAAqB,qBAArB;AAEA,eAAc;EACZ;EACCC,IAAI,GAAE;IACJ,OAAM;MACJC,IAAI,EAAE,CADF;MAEJC,KAAK,EAAE,CAFH;MAGJC,MAAM,EAAE,EAHJ;MAIHC,GAAG,EAAE,CAJF;MAKH;MACA;MACCC,KAAK,EAAE,EAPL;MAQFC,aAAa,EAAC,CAEP;QACIC,QAAQ,EAAC,qBADb;QAEIC,UAAU,EAAC,qBAFf;QAGIC,WAAW,EAAC,SAHhB;QAIIC,SAAS,EAAC,WAJd;QAKIC,QAAQ,EAAC,MALb;QAMIC,YAAY,EAAC,sGANjB;QAOIC,SAAS,EAAE,KAPf;QAQIC,YAAY,EAAE;MARlB,CAFO,EAYN;QACGP,QAAQ,EAAC,qBADZ;QAEGC,UAAU,EAAC,qBAFd;QAGGC,WAAW,EAAC,SAHf;QAIEC,SAAS,EAAC,WAJZ;QAKGC,QAAQ,EAAC,MALZ;QAMGC,YAAY,EAAC,sGANhB;QAOGC,SAAS,EAAE,KAPd;QAQGC,YAAY,EAAE;MARjB,CAZM,EAsBN;QACGP,QAAQ,EAAC,qBADZ;QAEGC,UAAU,EAAC,qBAFd;QAGGC,WAAW,EAAC,SAHf;QAIEC,SAAS,EAAC,WAJZ;QAKGC,QAAQ,EAAC,MALZ;QAMGC,YAAY,EAAC,sGANhB;QAOGC,SAAS,EAAE,KAPd;QAQGC,YAAY,EAAE;MARjB,CAtBM,EAgCN;QACGP,QAAQ,EAAC,qBADZ;QAEGC,UAAU,EAAC,qBAFd;QAGGC,WAAW,EAAC,SAHf;QAIEC,SAAS,EAAC,WAJZ;QAKGC,QAAQ,EAAC,MALZ;QAMGC,YAAY,EAAC,sGANhB;QAOGC,SAAS,EAAE,KAPd;QAQGC,YAAY,EAAE;MARjB,CAhCM,CARZ;MAkDHC,MAAM,EAAE,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,EAlDvB;MAmDHC,KAAK,EAAE;IAnDJ,CAAN;EAsDD,CAzDU;;EA0DX,MAAMC,OAAN,GAAg